#include <fstream>
#include <string>
#include <iostream>

int main(void)
{   
    std::string s;
    int n;
    
    std::cin>>n;
    std::cin>>s;
    
    std::string b;

    

    for (int i = 0; i<n; i++)
    {
        if (s[i] != '*')
        {
            std::cout<<s[i];
        }
        else
        {
            if(s[i+1] != '*')
            {
                std::cout<<' ';
            }
        }
    }

    return 0;
}
