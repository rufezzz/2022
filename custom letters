#include <fstream>
#include <string>
#include <iostream>

int main(void)
{
    char c;

    std::cin>>c;

    if (c == 'A' || c == 'D' || c == 'O' || c == 'P' || c == 'Q' || c == 'R')
    {
        std::cout<<'1';
    }
    else
    {
        if (c == 'B')
        {
            std::cout<<'2';
        }
        else
        {
            std::cout<<'0';
        }
    }
    return 0;
}
